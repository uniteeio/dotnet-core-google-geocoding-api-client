<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <Authors>FabienP, Unitee</Authors>
        <PackageId>Unitee.GoogleGeocoding.ApiClient</PackageId>
        <Version>1.0.0</Version>
        <NeutralLanguage>en-US</NeutralLanguage>
        <VersionPrefix>1.0.0</VersionPrefix>
        <Title>.NET Core Google Geocoding API client</Title>
        <Description>A .NET Core api client for Google geocoding API</Description>
        <PackageProjectUrl>https://www.nuget.org/packages/Unitee.GoogleGeocoding.ApiClient/</PackageProjectUrl>
        <RepositoryUrl>https://github.com/uniteeio/dotnet-core-google-geocoding-api-client</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Geocoding.Core" Version="4.0.1"/>
        <PackageReference Include="Geocoding.Google" Version="4.0.1"/>
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.1.1"/>
        <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="2.1.1"/>
    </ItemGroup>

</Project>